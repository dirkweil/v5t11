<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>/view/bausteinProgrammierung.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>openProgMode</from-outcome>
   <if>#{bausteinProgrammierungModel.currentBaustein.busBaustein}</if>
   <to-view-id>/view/bausteinProgrammierung_openBusProgMode.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>openProgMode</from-outcome>
   <if>#{!bausteinProgrammierungModel.currentBaustein.busBaustein}</if>
   <to-view-id>/view/bausteinProgrammierung_openGleisProgMode.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/view/bausteinProgrammierung*</from-view-id>
  <navigation-case>
   <from-outcome>closeProgMode</from-outcome>
   <if>#{bausteinProgrammierungModel.currentBaustein.busBaustein}</if>
   <to-view-id>/view/bausteinProgrammierung_closeBusProgMode.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>closeProgMode</from-outcome>
   <if>#{!bausteinProgrammierungModel.currentBaustein.busBaustein}</if>
   <to-view-id>/view/bausteinProgrammierung_closeGleisProgMode.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/view/bausteinProgrammierung_closeBusProgMode.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>bausteinProgrammierung</from-outcome>
   <to-view-id>/view/bausteinProgrammierung.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <factory>
  <exception-handler-factory>de.gedoplan.v5t11.betriebssteuerung.model.ViewExpiredExceptionHandlerFactory</exception-handler-factory>
  <exception-handler-factory>de.gedoplan.v5t11.betriebssteuerung.model.SelectrixExceptionHandlerFactory</exception-handler-factory>
 </factory>
</faces-config>
